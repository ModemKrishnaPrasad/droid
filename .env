# Firebase App ID for the staging environment (find this in Firebase console)
STAGE_APP_ID="com.example.droid_stage"  # Replace with your staging app's package ID
STAGE_VERSION_CODE="1"  # Version code for the staging build
STAGE_VERSION_NAME="1.0"  # Version name for the staging build
STAGE_FIREBASE_APP_ID="1:1234567890:android:abcdef1234567890"  # Replace with your Firebase App ID for staging
STAGE_TESTERS="example@test.com"  # Email(s) for testers for the staging app

# Firebase App ID for the production environment (find this in Firebase console)
PROD_APP_ID="com.example.droid"  # Replace with your production app's package ID
PROD_VERSION_CODE="2"  # Version code for the production build
PROD_VERSION_NAME="2.0"  # Version name for the production build
PROD_FIREBASE_APP_ID="1:1234567890:android:abcdef0987654321"  # Replace with your Firebase App ID for production
PROD_TESTERS="example@test.com"  # Email(s) for testers for the production app

# Firebase CLI token for authentication (generated using `firebase login:ci`)
FIREBASE_CLI_TOKEN="1//your_generated_token_here"  # Replace with your Firebase CLI token
